<div class="container">
  <div class="container">
    <h1><%= job.title %></h1>
    <img class="logo" src="<%= job.logo%>" alt="">
  </div>

  <h5> <%= jobDescription %></h5>
</div>

<div class="container">
  <h2> Your skills:</h2>

  <% skills.forEach(s => { %>
    <%= s.name %>
  <% }) %>
</div>

<div class="container">
  <h2> Job skills:</h2>

  <% jobSkills.forEach(j => { %>
    <%= j %>
  <% }) %>
</div>

<div class="container">
  <h2> Percentage:</h2>

  <%= percentage %>

</div>
